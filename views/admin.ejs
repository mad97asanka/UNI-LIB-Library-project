<!DOCTYPE html>
<html>
<head>
  <link rel="StyleSheet" href="/CSS/admin.css">
</head>
<body style="text-align: center;">
  <h1 class="form_title">Admin Account Details</h1>


  <table>
    <tr>
      <th>ID</th>
      <th>First Name</th>
      <th>Last Name</th>
      <th>Email</th>
      <th>Password</th>
      <th>Recovery Email</th>
    </tr>
    <% for (const doc of documents) { %>
      <tr>
        <td><%= doc.User_ID %></td>
        <td><%= doc.First_Name %></td>
        <td><%= doc.Last_Name %></td>
        <td><%= doc.Email %></td>
        <td><%= doc.Password %></td>
        <td><%= doc.Recovery_Email %></td>
      </tr>
    <% } %>
  </table>
</body>
</html>
